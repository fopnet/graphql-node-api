  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}\\node_modules\\mocha\\bin\\_mocha",
      "stopOnEntry": false,
      "args": ["${workspaceRoot}\\test\\user\\user.resolvers.test.ts"],
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": null,
      "runtimeArgs": ["--nolazy"],
      "env": {
        "NODE_ENV": "test"
      },
      "console": "integratedTerminal",
      "sourceMaps": false,
      "outFiles": []
    },
    {
      "name": "Attach",
      "type": "node",
      "request": "attach",
      "port": 5858,
      "sourceMaps": false,
      "outFiles": []
      "localRoot": "${workspaceRoot}",
      "remoteRoot": null
    }
  ]
}
